{
    "name": "@javascribble/quantum-engine",
    "description": "web based game engine",
    "version": "0.0.0",
    "license": "MIT",
    "dependencies": {},
    "devDependencies": {
        "@javascribble/quantum": "*",
        "@javascribble/quantum-tree": "*",
        "@javascribble/rollup-plugin-modulify": "*",
